{
    "name": "腾百万",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "演示content_scripts 的通信",
    "browser_action": {
        "default_title": "查看",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.baidu.com/*"],
            "js": ["jquery-2.1.4.min.js","baidu.js"]
        }
    ],
    // "background" : {
    //     "scripts": ["background.js"],
    //     "persistent": false
    // },
    "permissions" : ["tabs", "activeTab"] //向浏览器申请的权限
}